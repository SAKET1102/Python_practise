def add(a,b):
    return a + b
def sub(a,b):
    return a - b
def mul(a,b):
    return a * b
def div(a,b):
    return a / b

a = float(input("Enter 1st no "))
b = float(input("Enter 2nd no "))

print("1.Add\n2.Subtract\n3.Multiply\n4.Divide\n ")

c = int(input("Select from the list "))


if c == 1:
    print(add(a,b))
elif c == '2':
    print(sub(a,b))
elif c == '3':
    print(mul(a,b))
elif c == '4':
    print(div(a,b))
